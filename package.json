  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunchir/c7n-color-palette.gitissues"
  },
  "homepage": "https://github.com/sunchir/c7n-color-palette.git#readme",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/tinycolor2": "^1.4.2",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.13.2",
    "tinycolor2": "^1.4.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {}
